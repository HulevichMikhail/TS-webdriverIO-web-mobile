{
  "name": "web-mobile-testing",
  "version": "1.0.0",
  "author": "Mikhail Hulevich",
  "description": "This boilerplate implementation created by Mikhail Hulevich. Stub folders, taken from the official webdriverio page. Allows to run WebdriverIO tests: WEB and mobile (Android. iOS)",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "test:web": "wdio config/wdio.web.local.conf.ts",
    "test:android": "wdio config/wdio.android.app.conf.ts",
    "test:android.browser": "wdio config/wdio.android.browser.conf.ts",
    "test:ios": "wdio config/wdio.ios.app.conf.ts",
    "test:ios.browser": "wdio config/wdio.ios.browser.conf.ts",
    "test:report": "allure generate allure-results --clean --output reports/allure-report && allure open reports/allure-report"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@wdio/allure-reporter": "^8.11.0",
    "@wdio/appium-service": "^8.10.5",
    "@wdio/cli": "^8.10.2",
    "@wdio/globals": "^8.10.2",
    "@wdio/local-runner": "^8.10.2",
    "@wdio/mocha-framework": "^8.10.2",
    "allure-commandline": "^2.22.1",
    "appium": "^2.0.0-beta.71",
    "appium-uiautomator2-driver": "^2.25.1",
    "chai": "^4.3.7",
    "eslint": "^8.39.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "wdio-chromedriver-service": "^8.1.1",
    "webdriverio": "^8.10.2"
  }
}
